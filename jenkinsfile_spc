pipeline {
    agent {label 'Jenkins_node_setup'
     }
    stages {
        stage('check out scm') {
            steps {
                git url: 'https://github.com/spring-projects/spring-petclinic.git',
                branch: 'main'
           }
        }
        stage('build') {
            steps {
            sh './mvnw package'
            }
        }
        stage('Deploy') {
            steps {
              sh 'sudo nohup java -jar /home/ubuntu/jenkins/workspace/spring-petclinic@2/target/spring-petclinic-3.3.0-SNAPSHOT.jar.original'
            }
        }
    }
}
